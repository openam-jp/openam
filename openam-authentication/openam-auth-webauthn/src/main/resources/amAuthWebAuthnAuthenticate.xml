                    type="single"   
                    syntax="string"    
                    i18nKey="a650"
                    order="800"
                    resourceName="displayNameAttributeName">
                    <DefaultValues>
                        <Value>cn</Value> 
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-auth-Webauthn-auth-level"
                    type="single"
                    syntax="number_range" rangeStart="0" rangeEnd="2147483647"
                    i18nKey="a615"
                    order="900"
                    resourceName="authenticationLevel">
                    <DefaultValues>
                        <Value>0</Value>
                    </DefaultValues>
                </AttributeSchema>     
