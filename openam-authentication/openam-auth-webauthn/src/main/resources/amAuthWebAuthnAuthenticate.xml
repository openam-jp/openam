                        <DefaultValues>
                            <Value>preferred</Value> 
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-Webauthn-timeout"
                        type="single"   
                        syntax="string"    
                        i18nKey="a622"
                        order="402"
                        resourceName="timeout">
                        <DefaultValues>
                            <Value>60000</Value> 
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-Webauthn-useMfa"
                        type="single_choice"   
                        syntax="string"    
                        i18nKey="a623"
                        order="403"
                        resourceName="useMfa">
                        <ChoiceValues>
                            <ChoiceValue i18nKey="false">false</ChoiceValue>
                            <ChoiceValue i18nKey="true">true</ChoiceValue>
                        </ChoiceValues>
                        <DefaultValues>
                            <Value>false</Value> 
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-Webauthn-displayNameAttributeName"
                        type="single"   
                        syntax="string"    
                        i18nKey="a650"
                        order="800"
                        resourceName="displayNameAttributeName">
                        <DefaultValues>
                            <Value>cn</Value> 
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-Webauthn-auth-level"
                        type="single"
                        syntax="number_range" rangeStart="0" rangeEnd="2147483647"
                        i18nKey="a615"
                        order="900"
                        resourceName="authenticationLevel">
                        <DefaultValues>
                            <Value>0</Value>
                        </DefaultValues>
                    </AttributeSchema>   
                </SubSchema>         
            </Organization>
        </Schema>
    </Service>
</ServicesConfiguration>
