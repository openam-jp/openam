                            <ChoiceValue i18nKey="false">false</ChoiceValue>
                            <ChoiceValue i18nKey="true">true</ChoiceValue>
                        </ChoiceValues>
                        <DefaultValues>
                            <Value>false</Value> 
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-Webauthn-userVerification"
