                        syntax="string"    
                        i18nKey="a621"
                        order="401"
                        resourceName="userVerification">
                        <ChoiceValues>
                            <ChoiceValue i18nKey="preferred">preferred</ChoiceValue>
                            <ChoiceValue i18nKey="required">required</ChoiceValue>
                            <ChoiceValue i18nKey="discouraged">discouraged</ChoiceValue>
