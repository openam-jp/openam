                        type="single"   
                        syntax="string"    
                        i18nKey="a617"
                        order="101"
                        resourceName="origin">
                        <DefaultValues>
                            <Value>https://@SERVER_HOST@:443</Value> 
                        </DefaultValues>
                    </AttributeSchema>
