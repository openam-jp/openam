                    type="single"   
                    syntax="string"    
                    i18nKey="a616"
                    order="100"
                    resourceName="rp">
                    <DefaultValues>
                        <Value>OpenAM RP </Value> 
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-auth-Webauthn-origin"
