<!DOCTYPE ModuleProperties PUBLIC "=//iPlanet//Authentication Module Properties XML Interface 1.0 DTD//EN"
        "jar://com/sun/identity/authentication/Auth_Module_Properties.dtd">

<ModuleProperties moduleName="WebAuthnRegister" version="1.0" >

    <Callbacks length="0" order="1" timeout="300" header="認証デバイスの登録">
    </Callbacks>

    <Callbacks length="3" order="2" timeout="120" header="認証デバイスを操作してください">
        <TextOutputCallback messageType="script">PLACEHOLDER</TextOutputCallback>
        <HiddenValueCallback>
           <Id>PublicKeyCredential</Id>
        </HiddenValueCallback>
        <ConfirmationCallback>
            <OptionValues>
                <OptionValue>
                    <Value>次へ</Value>
                </OptionValue>
                <OptionValue>
                    <Value>キャンセル</Value>
                </OptionValue>
            </OptionValues>
        </ConfirmationCallback>
    </Callbacks>

    <Callbacks length="2" order="3" timeout="120" header="認証デバイスが登録されました識別名を入力してください（任意）">
        <NameCallback>
            <Prompt>鍵の識別名:</Prompt>
        </NameCallback>
        <ConfirmationCallback>
            <OptionValues>
                <OptionValue>
                    <Value>次へ</Value>
                </OptionValue>
            </OptionValues>
        </ConfirmationCallback>
    </Callbacks>

</ModuleProperties>
